gem 'turbolinks'
gem 'json', '1.8.1'
gem 'jbuilder', '~> 1.2'
gem 'pg', '>=0.17'
gem 'devise'
gem 'cancan'

group :doc do
  # bundle exec rake doc:rails generates the API under doc/api.
  gem 'sdoc', require: false
end

group :test, :development do
  gem 'sqlite3'
  gem 'rspec-rails', '>=3.0.1'
  gem 'factory_girl_rails'
end

