<div class="page-header">
  <h2>users</h2>
</div>
  <table class="table table-striped">
  <tr>
    <th>Id</th>
    <th>Last Name</th>
    <th>First Name</th>
    <th>Action</th>
  </tr>
  <% @users.each do |user| %>
   <tr>
    <td><%= link_to "#{user.id_string}", controller: "users", action: "show", id: user.id %></td>
    <td><%= "#{user.last_name}" %></td>
    <td><%= "#{user.first_name}" %></td>
    <td><%= link_to "Delete", @user, method: :delete, data: {confirm: 'Are you sure?'} %></td>
  </tr>
  <% end %>
</table>
<%= link_to "Create new user", controller: "users", action: "new" %>

