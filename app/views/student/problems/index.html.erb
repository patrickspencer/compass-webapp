<p id="notice"><%= notice %></p>

<h1>Listing Student Problems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @problems.each do |problem| %>
      <tr>
        <td>Problem id: <%= problem.id %></td>
        <td>user id: <%= problem.user_id %></td>
        <td>seed <%= problem.seed %></td>
        <td><%= link_to 'Show', student_problem_path(problem) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
