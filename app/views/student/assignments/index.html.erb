<h1>Assignments</h1>
<% @assignments.each {|b| print b} %>
<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Id</th>
    <th>Assignment Type ID</th>
    <th>Course ID</th>
  </tr>
  <% @assignments.each do |assignment| %>
  <tr>
    <td><%= link_to "#{assignment.name}", controller: "assignments", action: "show", id: assignment.id%></td>
    <td><%= assignment.id %></td>
    <td><%= assignment.assignment_type_id %></td>
    <td><%= assignment.course_id %></td>
  </tr>
  <% end %>
</table>
